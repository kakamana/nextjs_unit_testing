{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "security:unit": "jest --testPathPatterns=security-tests/unit",
    "security:sast": "eslint . --ext .ts,.tsx -f json -o artifacts/security/eslint.json",
    "security:zap": "docker run --rm --network host -v %CD%/artifacts/security:/zap/wrk owasp/zap2docker-stable zap-baseline.py -t http://localhost:3000 -r zap_report.html -J zap_report.json",
    "security:zap:local": "docker run --rm -v %CD%/artifacts/security:/zap/wrk owasp/zap2docker-stable zap-baseline.py -t http://host.docker.internal:3000 -r zap_report.html -J zap_report.json",
    "security:report": "node scripts/security-report.js"
  },
  "dependencies": {
    "lucide-react": "^0.541.0",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "puppeteer": "^23.8.1",
    "wait-on": "^7.2.0"
  }
}
